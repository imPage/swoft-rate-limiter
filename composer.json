{
    "name": "zcmzc/swoft-rate-limiter",
    "type": "library",
    "keywords": [
        "php",
        "swoft",
        "rateLimite"
    ],
    "description": "Swoft 限流器",
    "license": "Apache-2.0",
    "require": {
        "swoft/http-server": "^1.0",
        "swoft/framework": "^1.0",
        "sunspikes/php-ratelimiter": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\RateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\RateLimiter\\": "test/Cases"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}